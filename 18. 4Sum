class Solution {
    public List<List<Integer>> fourSum(int[] nums, int target) {
        List<List<Integer>> res = new ArrayList<>();
        Arrays.sort(nums);
        int n = nums.length;
        for(int i = 0 ; i < n; i++){
            if(i > 0 && nums[i] == nums[i-1]) continue;
            for(int j = i + 1 ; j < n; j++){
                 if(j-i > 1 && nums[j] == nums[j-1]) continue;
                //System.out.println("inner loop");
                int k = j + 1 , l = n - 1;
                while(k < l){
                    long fourSum = (long) nums[i] + nums[j] + nums[k] + nums[l];
                   // System.out.println(fourSum);
                    if(fourSum == target){
                        List<Integer> list = new ArrayList<>();
                        list.add(nums[i]);
                        list.add(nums[j]);
                        list.add(nums[k]);
                        list.add(nums[l]);
                        res.add(list);
                        k++;
                        while(k < l && nums[k] == nums[k-1]){
                            k++;
                        }
                    }
                    else if(fourSum < target) k++;
                    else l--;
                }
            }
        }
        return res;
    }
}
