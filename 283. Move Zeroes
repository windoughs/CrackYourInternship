class Solution {
    public void moveZeroes(int[] nums) {
        int i = 0 , j = -1;
        while(i < nums.length){
            if(nums[i] == 0){
                if(j == -1) j = i + 1;
                while(j < nums.length){
                    if(nums[j] != 0){
                        //swap
                        int temp = nums[i];
                        nums[i] = nums[j];
                        nums[j] = temp; 
                        break;
                    }
                    j++;
                }
            }
            i++;
        }
    }
}
